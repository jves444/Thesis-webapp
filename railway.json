{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "nixpacks": {
            "config": {
                "setupCmd": "python -m pip install --upgrade pip==23.1.2 setuptools==67.7.2 wheel==0.40.0",
                "buildCmd": "pip install --no-cache-dir -r requirements.txt"
            }
        }
    },
    "deploy": {
        "runtime": "PYTHON",
        "pythonVersion": "3.10.13",
        "numReplicas": 1
    }
}